<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="HardcodedText"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/mTv_title"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:textColor="@color/white"
        android:text="综合监管"
        android:textSize="@dimen/content"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@color/main_color"
         />

    <View
        android:id="@+id/no_use_top_view"
        android:layout_width="wrap_content"
        android:layout_height="10dp"
        android:background="@color/gray"
        app:layout_constraintTop_toBottomOf="@+id/mTv_title"/>

    <!--统计分析-->
    <TextView
        android:id="@+id/mTv_statistic"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/no_use_top_view"
        app:layout_constraintBottom_toTopOf="@+id/no_use_center_h_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/no_use_statistic_title_view"
        android:layout_width="2dp"
        android:layout_height="20dp"
        android:background="@color/main_color"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/mTv_statistic"
        app:layout_constraintTop_toTopOf="@+id/mTv_statistic"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:textColor="@color/textBlackN"
        android:text="统计分析"
        app:layout_constraintLeft_toRightOf="@+id/no_use_statistic_title_view"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/mTv_statistic"
        app:layout_constraintTop_toTopOf="@+id/mTv_statistic"/>

    <View
        android:id="@+id/no_use_view_1"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/gray"
        android:layout_margin="5dp"
        app:layout_constraintTop_toBottomOf="@+id/no_use_statistic_title_view"/>

    <ImageView
        android:id="@+id/mImg_chart"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="2"
        app:layout_constraintTop_toBottomOf="@+id/no_use_view_1"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_2"
        android:background="@drawable/home_count_chart"/>

    <View
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/gray"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_2"
        app:layout_constraintTop_toBottomOf="@+id/mImg_chart"/>
    <View
        android:id="@+id/no_use_view_2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mImg_chart"
        app:layout_constraintBottom_toTopOf="@+id/no_use_center_h_view"/>

    <ViewStub
        android:id="@+id/no_use_view_2_center_h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_2"
        app:layout_constraintBottom_toBottomOf="@+id/no_use_view_2"/>

    <!--自查上报分析-->
    <ImageView
        android:id="@+id/mImg_check_report"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        android:src="@drawable/home_img_self_check"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/mImg_supervise_count"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_2_center_h"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/content_nn"
        android:textColor="@color/textBlackN"
        app:layout_constraintTop_toBottomOf="@+id/mImg_check_report"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/mImg_supervise_count"
        android:text="自查上报分析"/>
    <!--综合监管分析-->
    <ImageView
        android:id="@+id/mImg_supervise_count"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        android:src="@drawable/home_img_supervise_count"
        app:layout_constraintLeft_toRightOf="@+id/mImg_check_report"
        app:layout_constraintRight_toLeftOf="@+id/mImg_project_count"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_2_center_h"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/content_nn"
        android:textColor="@color/textBlackN"
        app:layout_constraintTop_toBottomOf="@+id/mImg_supervise_count"
        app:layout_constraintLeft_toRightOf="@+id/mImg_check_report"
        app:layout_constraintRight_toLeftOf="@+id/mImg_project_count"
        android:text="综合监管分析"/>
    <!--评分项目分析-->
    <ImageView
        android:id="@+id/mImg_project_count"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        android:src="@drawable/home_img_project_count"
        app:layout_constraintLeft_toRightOf="@+id/mImg_supervise_count"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_2_center_h"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/content_nn"
        android:textColor="@color/textBlackN"
        app:layout_constraintTop_toBottomOf="@+id/mImg_project_count"
        app:layout_constraintLeft_toRightOf="@+id/mImg_supervise_count"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="评分项目分析"/>
    <View
        android:id="@+id/no_use_center_h_view"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:background="@color/gray"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <!--监管预警-->
    <View
        android:id="@+id/no_use_view_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/no_use_center_h_view"
        />
    <View
        android:id="@+id/no_use_warning_title_view"
        android:layout_width="2dp"
        android:layout_height="20dp"
        android:background="@color/main_color"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_3"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_3"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:textColor="@color/textBlackN"
        android:text="监管预警"
        app:layout_constraintLeft_toRightOf="@+id/no_use_warning_title_view"
        android:layout_marginStart="15dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_3"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_3"/>

    <View
        android:id="@+id/no_use_view_4"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/gray"
        android:layout_margin="5dp"
        app:layout_constraintTop_toBottomOf="@+id/no_use_warning_title_view"/>

    <ViewStub
        android:id="@+id/no_use_warning_center_h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_4"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <ViewStub
        android:id="@+id/no_use_warning_center_h4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_4"
        app:layout_constraintBottom_toBottomOf="@+id/no_use_warning_center_h"/>
    <!--自查预警-->
    <ImageView
        android:id="@+id/mImg_warning_check"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/count_img_check_warning"
        app:layout_constraintRight_toLeftOf="@+id/mImg_warning_supervise"
        app:layout_constraintBottom_toTopOf="@+id/no_use_warning_center_h4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textBlackN"
        android:textSize="@dimen/content_nn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mImg_warning_check"
        app:layout_constraintRight_toLeftOf="@+id/mImg_warning_supervise"
        android:text="自查预警"/>
    <!--监管预警-->
    <ImageView
        android:id="@+id/mImg_warning_supervise"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        android:src="@drawable/count_img_supervise_warning"
        app:layout_constraintLeft_toRightOf="@+id/mImg_warning_check"
        app:layout_constraintRight_toLeftOf="@+id/mImg_warning_honesty"
        app:layout_constraintBottom_toTopOf="@+id/no_use_warning_center_h4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textBlackN"
        android:textSize="@dimen/content_nn"
        app:layout_constraintTop_toBottomOf="@+id/mImg_warning_check"
        app:layout_constraintLeft_toRightOf="@+id/mImg_warning_check"
        app:layout_constraintRight_toLeftOf="@+id/mImg_warning_honesty"
        android:text="监管预警"/>
    <!--诚信预警-->
    <ImageView
        android:id="@+id/mImg_warning_honesty"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintHorizontal_weight="1"
        android:src="@drawable/count_img_honesty"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/mImg_warning_supervise"
        app:layout_constraintBottom_toTopOf="@+id/no_use_warning_center_h4"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textBlackN"
        android:textSize="@dimen/content_nn"
        app:layout_constraintTop_toBottomOf="@+id/mImg_warning_check"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/mImg_warning_supervise"
        android:text="诚信预警"/>

    <View
        android:id="@+id/no_use_view_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/shadow"
        android:layout_marginBottom="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/no_use_warning_center_h"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no_use_warning_center_h"/>

    <!--单位模块-->
    <TextView
        android:id="@+id/mTv_bottom_unit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="自查预警高风险前十"
        android:gravity="center_vertical"
        android:paddingStart="8dp"
        android:textSize="@dimen/content_nn"
        android:textColor="@color/textBlack"
        app:layout_constraintVertical_weight="2"
        app:layout_constraintBottom_toTopOf="@+id/no_use_view_6"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_5"
        app:layout_constraintEnd_toEndOf="@+id/no_use_view_5"
        app:layout_constraintTop_toTopOf="@+id/no_use_view_5"
        />

    <View
        android:id="@+id/no_use_view_6"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:background="@color/gray"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/mTv_bottom_unit"
        app:layout_constraintBottom_toTopOf="@+id/no_use_bottom_unit_bottom"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_5"
        app:layout_constraintEnd_toEndOf="@+id/no_use_view_5"/>
    <TextView
        android:id="@+id/no_use_bottom_unit_bottom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="3"
        app:layout_constraintTop_toBottomOf="@+id/no_use_view_6"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_5"
        app:layout_constraintEnd_toEndOf="@+id/no_use_view_5"
        app:layout_constraintBottom_toBottomOf="@+id/no_use_view_5"
        />
    <ImageView
        android:id="@+id/no_use_img_warning"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/count_img_warning"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_5"
        app:layout_constraintTop_toTopOf="@+id/no_use_bottom_unit_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/no_use_view_5"/>

    <ViewStub
        android:id="@+id/no_use_waring_center_h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/no_use_view_5"
        app:layout_constraintTop_toTopOf="@+id/no_use_bottom_unit_bottom"
        app:layout_constraintBottom_toBottomOf="@+id/no_use_view_5"/>

    <TextView
        android:id="@+id/mTv_warning_unit_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/no_use_waring_center_h"
        app:layout_constraintLeft_toRightOf="@+id/no_use_img_warning"
        android:layout_marginStart="15dp"
        android:textColor="@color/textBlack"
        android:textSize="@dimen/content_nn"
        android:text="甘肃省中医院"/>

    <TextView
        android:id="@+id/mTv_warning_unit_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/no_use_waring_center_h"
        app:layout_constraintLeft_toRightOf="@+id/no_use_img_warning"
        android:layout_marginStart="15dp"
        android:textColor="@color/textBlackN"
        android:textSize="@dimen/content_nn"
        android:text="地址：兰州市人民路345号"/>

</android.support.constraint.ConstraintLayout>